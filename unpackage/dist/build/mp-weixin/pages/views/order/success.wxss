.jieguo{width:100%;height:100%;position:fixed;top:0;bottom:0;padding:0 8%;overflow:hidden}.place{position:absolute;width:100%;height:100%;top:0;left:0;background:-webkit-gradient(linear,left top,left bottom,from(hsla(0,0%,100%,0)),color-stop(hsla(0,0%,100%,.3)),color-stop(hsla(0,0%,100%,.5)),to(#fff));background:-webkit-linear-gradient(hsla(0,0%,100%,0),hsla(0,0%,100%,.3),hsla(0,0%,100%,.5),#fff);background:linear-gradient(hsla(0,0%,100%,0),hsla(0,0%,100%,.3),hsla(0,0%,100%,.5),#fff);z-index:10}.jieguo_box{width:88%;background-color:#fff;height:90vw;-webkit-border-radius:10rpx;border-radius:10rpx;-webkit-box-shadow:0rpx 0rpx 10rpx #999;box-shadow:0rpx 0rpx 10rpx #999;padding:20rpx;z-index:900;position:absolute;top:150rpx;left:50%;-webkit-transform:translateX(-50%);transform:translateX(-50%)}.success{text-align:center;margin-top:50rpx}.success text{font-size:140rpx}.jieguo .texts, .moneys{text-align:center;font-size:32rpx;margin-top:20rpx;font-weight:700}.wancheng{width:80%;height:80rpx;border:2rpx solid #3e7e8b;color:#3e7e8b;text-align:center;line-height:80rpx;-webkit-border-radius:10rpx;border-radius:10rpx;margin:0 auto;margin-top:150rpx;font-weight:700}.ewm{margin-top:20rpx;height:40rpx;line-height:40rpx;text-align:center;font-size:24rpx;color:#999}